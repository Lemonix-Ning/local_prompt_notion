{
  "name": "local-prompt-notion",
  "version": "1.0.0",
  "type": "module",
  "description": "本地优先的提示词管理系统",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite --port 3002 --strictPort",
    "dev:server": "npm --prefix server start",
    "dev:api": "concurrently -k -n server,client -c blue,green \"npm run dev:server\" \"npm run dev:client\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx"
  },
  "keywords": [
    "prompt",
    "management",
    "local-first",
    "notion-like"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "lucide-react": "^0.294.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.7"
  }
}

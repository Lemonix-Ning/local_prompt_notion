{
  "name": "local-prompt-notion",
  "version": "1.0.0",
  "type": "module",
  "description": "本地优先的提示词管理系统",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "dev:client": "vite --port 3000 --strictPort",
    "dev:server": "set PORT=3001 && set VAULT_PATH=%cd%\\vault && npm --prefix server start",
    "dev:api": "concurrently -k -n server,client -c blue,green \"npm run dev:server\" \"npm run dev:client\"",
    "seed:vault": "node scripts/seed-vault.cjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx",
    "cleanup": "node scripts/cleanup-temp-folders.cjs",
    "check:desktop": "node scripts/check-desktop-build.cjs",
    "tauri": "tauri",
    "build:sidecar": "node scripts/build-sidecar.mjs",
    "desktop:dev": "npm run tauri dev",
    "desktop:build": "npm run tauri build"
  },
  "keywords": [
    "prompt",
    "management",
    "local-first",
    "notion-like"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^2.9.1",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^2.9.6",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@yao-pkg/pkg": "^5.13.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.3.3",
    "vite": "^5.0.7"
  }
}
